<div class="col-xs-8 col-xs-offset-4" id="login">
<h1>Login</h1>
<%= form_for @artist, url: "/sessions", method: "post" do |f| %>
  <div>
    <%= f.text_field :email, placeholder: "Email" %>
  </div>
  <div>
    <%= f.password_field :password, placeholder: "Password" %>
  </div>
  <%= f.submit "Sign In", id: "submit" %>
<% end %>
</div>