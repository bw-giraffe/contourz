
<div class="col-xs-8 col-xs-offset-4" id="signup">
  <h1>Sign Up</h1>

  <%= form_for @artist do |f| %>
    <div>
      <%= f.text_field :username, placeholder: "Pick a username" %>
    </div>
    <div>
      <%= f.text_field :location, placeholder: "Location" %>
    </div>
    <div>
      <%= f.text_field :email, placeholder: "Email" %>
    </div>
    <div>
      <%= f.password_field :password, placeholder: "Password" %>
    </div>
    <div>
    <%= f.submit "Sign Up", id:"submit" %>
  <% end %>
</div>
