<h1>All of Your Drawings</h1>

<div class = "row">
	<% @drawings.each do | d | %>
	<div class = "col-xs-12 col-xs-offset-2 col-sm-3 col-sm-offset-2 col-md-3 col-md-offset-2 col-lg-3 col-lg-offset-2" id = "drawingGrid">
	</div>
	<%= link_to image_tag(d.image.url), art_path(d.id) %>
	<% end %>


</div>